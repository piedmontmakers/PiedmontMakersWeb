<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^/mini-maker-faire$ /school-maker-faire [L,R]
RewriteRule ^/mini-maker-faire/$ /school-maker-faire/ [L,R]
